<test name="ColonInFileNameStackTrace" expectedWarnings="0">
  <obfuscated>
    <line>  at a.s(:foo::bar:1)</line>
    <line>  at a.t(:foo::bar:)</line>
  </obfuscated>
  <mapping>
    <line>some.Class -&gt; a:\n</line>
  </mapping>
  <retraced>
    <line>  at some.Class.strawberry(Class.kt:99)</line>
    <line>  at some.Class.passionFruit(Class.kt:121)</line>
  </retraced>
  <retracedVerbose>
    <line>  at some.Class.int strawberry(int)(Class.kt:99)</line>
    <line>  at some.Class.int passionFruit(float)(Class.kt:121)</line>
  </retracedVerbose>
</test>